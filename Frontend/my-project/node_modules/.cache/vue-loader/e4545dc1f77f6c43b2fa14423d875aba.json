      username: '',\r\n      password: ''\r\n    }\r\n  },\r\n  beforeCreate: function () {\r\n    document.body.classList.add('signin')\r\n  },\r\n  methods: {\r\n    login_fun: async function () {\r\n      if (this.username !== '' && this.password !== '') {\r\n        await axios.get('http://localhost:8080/Project/REST-API/login?username=' + this.username)\r\n          .then(response => {\r\n            console.log(response.data)\r\n            // console.log(response.data.length);\r\n            // if(response.data.length >= 1) {\r\n            if (response.data == null) {\r\n              alert('Username or Password is incorrect')\r\n            // eslint-disable-next-line eqeqeq\r\n            } else if (response.data.username == this.username && response.data.password == this.password) {\r\n              alert('Signin Successfull')\r\n              router.push('/doctor/' + response.data.doctorID + '/home')\r\n              // router.push({name: 'SignIn'});\r\n            } else {\r\n              alert('Username or Password is incorrect')\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.log(error)\r\n          })\r\n      } else {\r\n        alert('A username and password must be present')\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style src=\"../../static/css/signin.css\" scoped></style>\r\n"]}]}